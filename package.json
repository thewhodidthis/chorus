{
  "name": "@thewhodidthis/trigger",
  "version": "0.0.0",
  "description": "General purpose envelope generator",
  "main": "index.js",
  "module": "index.mjs",
  "homepage": "https://github.com/thewhodidthis/trigger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/trigger.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/trigger/issues"
  },
  "scripts": {
    "pretest": "eslint test.js",
    "test": "browserify test | kpow",
    "prestart": "eslint index.mjs",
    "start": "rollup -f cjs index.mjs | buble > index.js",
    "prenexample": "eslint example/index.es",
    "example": "cd example; rollup -c node:x -f iife index.es | buble > index.js",
    "postnexample": "open example/index.html"
  },
  "eslintConfig": {
    "extends": "@thewhodidthis"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "@thewhodidthis/binocular": "github:thewhodidthis/binocular",
    "@thewhodidthis/eslint-config": "latest",
    "@thewhodidthis/sound": "github:thewhodidthis/sound",
    "browserify": "latest",
    "buble": "latest",
    "eslint": "latest",
    "kpow": "latest",
    "rollup": "latest",
    "rollup-config-x": "latest",
    "tape": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
